{
  "name": "dev-buddy-openai",
  "displayName": "Dev-Buddy-OpenAI",
  "description": "Dev Buddy OpenAI integrates with the OpenAI API to help developers write code faster by completing code comments like `TODO:` and `FIXME:`.",
  "publisher": "martouta",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/martouta/dev-buddy-openai.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dev-buddy-openai.completeComments",
        "title": "Dev Buddy OpenAI -> Complete Comments"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.2",
    "@types/vscode": "^1.75.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "jest": "^29.4.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "openai": "^3.1.0"
  },
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|ts)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/test/*.test.(js|ts)"
    ]
  }
}
